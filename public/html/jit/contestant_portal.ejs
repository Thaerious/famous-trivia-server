    <template id="text-input-template">
    <link rel="stylesheet" href="styles/generated/text_input.css">
    <div class="text-input">
        <nidget-text id="content" class="text" contenteditable=""></nidget-text>
        <div id="hint" class="hint-text">Enter Name Here</div>
    </div>
</template>

    
    <template id="nidget-button-template">
    <link rel="stylesheet" href="styles/generated/nidget_button.css">
    <div class="mask"></div>
    <div id="outer" class="button">
        <div id="text" class="text"></div>
    </div>
</template>

    
    <template id="player-container-template" data-include="player-panel">
    <link rel="stylesheet" href="styles/generated/player_container.css">

    <div class="outer hide-extra">
        <nidget-element id="expand-button" class="hidden">
            <div class="text">&#9654;</div>
        </nidget-element>
        <nidget-element style="--index: 0" class="inner first" data-index="0">
        </nidget-element>
    </div>

</template>

    
    <template id="player-panel-template">
    <link rel="stylesheet" href="styles/generated/player_panel.css">

    <div class="outer">
        <nidget-text id="name">Name</nidget-text>
        <nidget-text id="score">1000</nidget-text>
        <div id="clock">
            <div id="buzz-light"></div>
            <div class="clock-tick" data-index="0"></div>
            <div class="clock-tick" data-index="1"></div>
            <div class="clock-tick" data-index="2"></div>
            <div class="clock-tick" data-index="3"></div>
            <div class="clock-tick" data-index="4"></div>
        </div>
    </div>

</template>

    
    <template id="question-pane-template">
    <link rel="stylesheet" href="styles/generated/question_pane.css">

    <div id="outer">
        <nidget-text class="text-contents" id="text-contents" contenteditable="true"></nidget-text>
    </div>
</template>

    
    <template id="trivia-timer-template">
    <link rel="stylesheet" href="styles/generated/trivia_timer.css">
    <div class="bg"></div>
    <div id="outer" class="outer">
        <div class="inner"></div>
        <div class="inner" id="prev-tick"><div class="text">0</div></div>
        <div class="inner" id="current-tick"><div class="text">0</div></div>
        <div class="inner"></div>
    </div>
    <div class="mask"></div>
</template>


<div class="bg"></div>

<div id="page-container" class="aspect-fit">
    <div class="top-bar" id="top-bar">
        <menu-container image-src="assets/images/menu.png">
            <menu-item event-name="logout">Logout</menu-item>
        </menu-container>
        <nidget-text class="round-number-text">Round 1</nidget-text>
    </div>

    <div id="main-container">
        <player-container id="player-container-left"></player-container>
        <player-panel id="self-panel"></player-panel>

        <question-pane id="question-pane" class="hidden" template-id="question-pane-template"></question-pane>
        <game-board id="game-board" class="hidden"></game-board>
        <multiple-choice-present id="multiple-choice-present" class="hidden"></multiple-choice-present>

        <div class="button-container">
            <trivia-timer class="hidden"></trivia-timer>
            <nidget-button id="buzz" class="round raised center button">BUZZ</nidget-button>
        </div>
    </div>
</div>

