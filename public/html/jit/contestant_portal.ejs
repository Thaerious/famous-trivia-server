<template id="multiple-choice-present-template">
    <link rel="stylesheet" href="styles/generated/multiple_choice_present.css">
    <div class="outer">
        <div class="inner" data-index="0" style="--index: 0">
            <check-box checked="false" template-id="check-box-template"></check-box>
            <text-input tabindex="0" class="wager style-dotted" spellcheck="false" filter="[0-9]{0,4}">0</text-input>
            <nidget-text class="answer"></nidget-text>
        </div>

        <div class="inner" data-index="1" style="--index: 1">
            <check-box checked="false" template-id="check-box-template"></check-box>
            <text-input tabindex="1" class="wager style-dotted" spellcheck="false" filter="[0-9]{0,4}">0</text-input>
            <nidget-text class="answer"></nidget-text>
        </div>

        <div class="inner" data-index="2" style="--index: 2">
            <check-box checked="false" template-id="check-box-template"></check-box>
            <text-input tabindex="2" class="wager style-dotted" spellcheck="false" filter="[0-9]{0,4}">0</text-input>
            <nidget-text class="answer"></nidget-text>
        </div>

        <div class="inner" data-index="3" style="--index: 3">
            <check-box checked="false" template-id="check-box-template"></check-box>
            <text-input tabindex="3" class="wager style-dotted" spellcheck="false" filter="[0-9]{0,4}">0</text-input>
            <nidget-text class="answer"></nidget-text>
        </div>

        <div class="inner" data-index="4" style="--index: 4">
            <check-box checked="false" template-id="check-box-template"></check-box>
            <text-input tabindex="4" class="wager style-dotted" spellcheck="false" filter="[0-9]{0,4}">0</text-input>
            <nidget-text class="answer"></nidget-text>
        </div>

        <div class="inner" data-index="5" style="--index: 5">
            <check-box checked="false" template-id="check-box-template"></check-box>
            <text-input tabindex="5" class="wager style-dotted" spellcheck="false" filter="[0-9]{0,4}">0</text-input>
            <nidget-text class="answer"></nidget-text>
        </div>

        <nidget-element class="inner bonus" style="--index: 6">
            <nidget-text class="amount">000</nidget-text>
            <nidget-text class="label">BONUS</nidget-text>
        </nidget-element>

        <nidget-element class="inner total" style="--index: 7">
            <nidget-text class="amount">000</nidget-text>
            <nidget-text class="label">TOTAL</nidget-text>
