<%- include('check-box'); %>

<!--suppress ALL -->
<template id="multiple-choice-compose-template">
    <link rel="stylesheet" href="styles/generated/multiple_choice_compose.css">
    <div class="outer">
        <div class="choices">
            <nidget-radio-group id="radio-group" class="column left">
                <div class="row" data-index="0" style="--index: 0"><nidget-radio-button selected="true" id="radio-0"/></div>
                <div class="row" data-index="1" style="--index: 0"><nidget-radio-button id="radio-1"/></div>
                <div class="row" data-index="2" style="--index: 0"><nidget-radio-button id="radio-2"/></div>
                <div class="row" data-index="3" style="--index: 0"><nidget-radio-button id="radio-3"/></div>
                <div class="row" data-index="4" style="--index: 0"><nidget-radio-button id="radio-4"/></div>
            </nidget-radio-group>
            <div class="column right">
                <div class="row" data-index="0" style="--index: 0"><text-input id="txt-0" hint="enter answer" /></div>
                <div class="row" data-index="1" style="--index: 0"><text-input id="txt-1" hint="enter answer" /></div>
                <div class="row" data-index="2" style="--index: 0"><text-input id="txt-2" hint="enter answer" /></div>
                <div class="row" data-index="3" style="--index: 0"><text-input id="txt-3" hint="enter answer" /></div>
                <div class="row" data-index="4" style="--index: 0"><text-input id="txt-4" hint="enter answer" /></div>
            </div>
        </div>
    </div>
</template>