<%- include('check-box'); %>

<template id="multiple-choice-pane-template">
    <link rel="stylesheet" href="styles/generated/multiple_choice_pane.css">
    <div class="outer">
        <check-box style="--index: 0" data-index="0" checked="true" template-id="check-box-template"></check-box>
        <div class="answer" style="--index: 0">
            <nidget-text data-index="0" contenteditable="true"></nidget-text>
        </div>
        <check-box style="--index: 1" data-index="1" checked="true" template-id="check-box-template"></check-box>
        <div class="answer" style="--index: 1">
            <nidget-text data-index="1" contenteditable="true"></nidget-text>
        </div>
        <check-box style="--index: 2" data-index="2" checked="true" template-id="check-box-template"></check-box>
        <div class="answer" style="--index: 2">
            <nidget-text data-index="2" contenteditable="true"></nidget-text>
        </div>
        <check-box style="--index: 3" data-index="3" checked="true" template-id="check-box-template"></check-box>
        <div class="answer" style="--index: 3">
            <nidget-text data-index="3" contenteditable="true"></nidget-text>
        </div>
        <check-box style="--index: 4" data-index="4" checked="true" template-id="check-box-template"></check-box>
        <div class="answer" style="--index: 4">
            <nidget-text data-index="4" contenteditable="true"></nidget-text>
        </div>
        <check-box style="--index: 5" data-index="5" checked="true" template-id="check-box-template"></check-box>
        <div class="answer" style="--index: 5">
            <nidget-text data-index="5" contenteditable="true"></nidget-text>
        </div>
    </div>
</template>