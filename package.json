{
  "name": "trivia-server",
  "version": "0.1.0",
  "description": "Creator and Luancher for Online Trivia",
  "main": "src/server/server.js",
  "scripts": {
    "build-js": "browserify src/client/home.js -o public/scripts/home.js -d",
    "watch-js": "watchify src/client/home.js -o public/scripts/home.js -dv",
    "build-css": "sass src/styles/mobile/main.scss:public/styles/mobile.css src/styles/desktop/main.scss:public/styles/desktop.css src/styles/contestant.scss:public/styles/contestant.css",
    "watch-css": "sass --watch src/styles/mobile/main.scss:public/styles/mobile.css src/styles/desktop/main.scss:public/styles/desktop.css src/styles/contestant.scss:public/styles/contestant.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thaerious/trivia-server.git"
  },
  "author": "",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Thaerious/trivia-server/issues"
  },
  "homepage": "https://github.com/Thaerious/trivia-server#readme",
  "dependencies": {
    "@thaerious/nidget": "file:../nidget",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "helmet": "^4.4.1",
    "http": "^0.0.1-security"
  }
}
