{
  "name": "trivia-server",
  "version": "0.1.0",
  "description": "Creator and Luancher for Online Trivia",
  "main": "src/server/server.js",
  "scripts": {
    "home-build-js": "browserify -t babelify src/client/home.js -o public/scripts/home.js -d",
    "home-watch-js": "watchify -t babelify src/client/home.js -o public/scripts/home.js -dv",
    "editor-build-js": "browserify -t babelify src/client/editor.js -o public/scripts/editor.js -d",
    "editor-watch-js": "watchify -t babelify src/client/editor.js -o public/scripts/editor.js -dv",
    "build-js": "npm run home-build-js && npm run editor-build-js",
    "watch-js": "start npm run home-watch-js && start npm run editor-watch-js",
    "build-css": "sass src/styles:public/styles/generated/,",
    "watch-css": "sass --watch src/styles:public/styles/generated/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thaerious/trivia-server.git"
  },
  "author": "",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Thaerious/trivia-server/issues"
  },
  "homepage": "https://github.com/Thaerious/trivia-server#readme",
  "dependencies": {
    "@thaerious/nidget": "file:..\\..\\nidget",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "helmet": "^4.4.1",
    "http": "^0.0.1-security",
    "@babel/runtime": "^7.13.10"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "babelify": "^10.0.0"
  }
}
