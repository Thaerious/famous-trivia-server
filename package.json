{
  "name": "trivia-server",
  "version": "0.1.0",
  "description": "Creator and Luancher for Online Trivia",
  "main": "src/server/server.js",
  "scripts": {
    "home-build-js": "browserify src/client/home.js -o public/scripts/home.js -d",
    "home-watch-js": "watchify src/client/home.js -o public/scripts/home.js -dv",
    "editor-build-js": "browserify src/client/editor.js -o public/scripts/editor.js -d",
    "editor-watch-js": "watchify src/client/editor.js -o public/scripts/editor.js -dv",
    "editor-build-css": "sass src/styles/editor.scss:public/styles/generated/editor.css",
    "editor-watch-css": "sass --watch src/styles/editor.scss:public/styles/generated/editor.css",
    "build-js": "npm run home-build-js; npm run editor-build-js",
    "watch-js": "npm run home-watch-js; npm run editor-watch-js",
    "editor-watch": "npm run editor-watch-js; npm run editor-watch-css",
    "game-board-build-css": "sass src/styles/game_board.scss:public/styles/generated/game_board.css",
    "game-board-watch-css": "sass --watch src/styles/game_board.scss:public/styles/generated/game_board.css",
    "mc-build-css": "sass src/styles/multiple_choice_pane.scss:public/styles/generated/multiple_choice_pane.css",
    "mc-watch-css": "sass --watch src/styles/multiple_choice_pane.scss:public/styles/generated/multiple_choice_pane.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thaerious/trivia-server.git"
  },
  "author": "",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Thaerious/trivia-server/issues"
  },
  "homepage": "https://github.com/Thaerious/trivia-server#readme",
  "dependencies": {
    "@thaerious/nidget": "file:..\\..\\nidget",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "helmet": "^4.4.1",
    "http": "^0.0.1-security"
  }
}
