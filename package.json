{
  "name": "trivia-server",
  "version": "0.1.0",
  "description": "Creator and Luancher for Online Trivia",
  "main": "src/server/server.js",
  "scripts": {
    "editor-js": "browserify -t babelify src/client/editor.js -o public/scripts/generated/editor.js -d",
    "host-js": "browserify -t babelify src/client/host.js -o public/scripts/generated/host.js -d",
    "host-portal-js": "browserify -t babelify src/client/host_portal.js -o public/scripts/generated/host_portal.js -d",
    "index-js": "browserify -t babelify src/client/index.js -o public/scripts/generated/index.js -d",
    "launch-js": "browserify -t babelify src/client/launch_console.js -o public/scripts/generated/launch_console.js -d",
    "build-js": "npm run editor-js && npm run host-js && npm run launch-js",
    "watch-editor-js": "watchify -t babelify src/client/editor.js -o public/scripts/generated/editor.js -dv",
    "watch-launch-js": "watchify -t babelify src/client/launch_console.js -o public/scripts/generated/launch_console.js -dv",
    "watch-host-js": "watchify -t babelify src/client/host.js -o public/scripts/generated/host.js -dv",
    "watch-host-portal-js": "watchify -t babelify src/client/host_portal.js -o public/scripts/generated/host_portal.js -dv",
    "watch-index-js": "watchify -t babelify src/client/index.js -o public/scripts/generated/index.js -dv",
    "watch-js": "start npm run host-watch-js && start npm run editor-watch-js",
    "build-css": "sass src/styles:public/styles/generated/",
    "watch-css": "sass --watch src/styles:public/styles/generated/",
    "build": "npm run build-js && npm run build-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thaerious/trivia-server.git"
  },
  "author": "",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Thaerious/trivia-server/issues"
  },
  "homepage": "https://github.com/Thaerious/trivia-server#readme",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@thaerious/nidget": "file:..\\..\\nidget",
    "body-parser": "^1.19.0",
    "browserify": "^17.0.0",
    "cookie-parser": "^1.4.5",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "google-auth-library": "^7.0.3",
    "helmet": "^4.4.1",
    "http": "^0.0.1-security",
    "jsdom": "^16.5.3",
    "path": "^0.12.7",
    "sqlite3": "^5.0.2",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "babelify": "^10.0.0"
  }
}
